
@{
    ViewBag.Title = "Index";
}

@section Styles {
    <style>
        header {
            margin-top: 100px;
        }
    </style>
}

<header>@ViewBag.Title</header>

<div ng-controller="TestCtrl" class="container">

    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">55688</span>
        </div>
        <input ng-model="name" type="text" class="form-control" placeholder="Username" aria-label="Username" aria-describedby="basic-addon1">
    </div>

    <button type="button" ng-click="downloadMyPage()" class="btn btn-primary">Primary</button>

</div>

@section Scripts {
    <script src="https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/2.0.5/FileSaver.min.js"></script>
