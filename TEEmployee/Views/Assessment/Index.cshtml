@{
    ViewBag.Title = "Assessment";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>SelfAssessmentsStore</h2>

<div ng-controller="IndexCtrl">

    <table class="table">
        <tr>
            <th>ID</th>
            <th>CategoryId</th>
            <th>Content</th>
        </tr>

        <tr ng-repeat="item in SelfAssessments">
            <td ng-bind="item.Id"></td>
            <td ng-bind="item.CategoryId"></td>
            <td ng-bind="item.Content"></td>
        </tr>

    </table>

</div>



@section Scripts {
    <script src="~/Scripts/WebApp/Assessment/IndexCtrl.js"></script>
}
