<div ng-controller="ManagerOptionCtrl" class="container" style="margin-top: 100px">
    @*@if (Session["empno"] is "4125")
    {
        <input type="submit" value="設定" ng-click="SetManager()" class="btn btn-warning" />
    }*@
    <label> 請選擇要評核的主管 </label>
    <div class="row">
        <div class="col-sm-3" ng-repeat="item in GetScorePeople">
            <div class="card border-primary mb-3" style="max-width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title">{{item.name}} {{item.dutyName}}</h5>
                    <p class="card-text">{{item.empno}}</p>
                    <input type="submit" value="填問卷" ng-click="ManagerSuggest(item)" class="btn btn-warning" />
                </div>
            </div>
        </div>
    </div>
</div>